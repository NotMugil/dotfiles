# Set ZSH Directory
export ZSH="$HOME/.oh-my-zsh"

# Set ZSH Theme
ZSH_THEME="robbyrussell"
 
# Source ZSH Syntax highlighting catppuccin mocha theme
source ~/.config/zsh/.zsh/catppuccin_mocha-zsh-syntax-highlighting.zsh

# Plugins
plugins=(git zsh-autosuggestions zsh-syntax-highlighting)

# Source OHMYZSH.SH
source ~/.config/zsh/.oh-my-zsh/oh-my-zsh.sh

